cmake_policy(PUSH)
cmake_policy(SET CMP0057 NEW)
get_filename_component(svf_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)
 list(APPEND CMAKE_MODULE_PATH ${svf_CMAKE_DIR})
find_dependency(LLVM 6.0 REQUIRED CONFIG)
 list(REMOVE_AT CMAKE_MODULE_PATH -1)
 set(_supported_components Svf Cudd)
 foreach(_comp ${svf_FIND_COMPONENTS})
    if (NOT _comp IN_LIST _supported_components)
        message(STATUS "component not found is: ${_comp}")
        message(STATUS "supported components are: ${_supported_components}")
        set(svf_FOUND False)
        set(svf_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
    endif()
    include("${CMAKE_CURRENT_LIST_DIR}/svf${_comp}Targets.cmake")
endforeach()
 set(svf_LIBRARIES svf::Svf svf::Cudd)
cmake_policy(POP)
